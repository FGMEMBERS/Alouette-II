<!-- Panneau frein de rotor -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Parts/Rotor-brake/rotor-brake.xml</path>
    <offsets>
      <x-m> -0.19370 </x-m>
      <y-m>  0.11601 </y-m>
      <z-m> -0.16133 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

</PropertyList>
