<?xml version="1.0"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <object-name>panel1</object-name>
    <object-name>panel3</object-name>
    <object-name>panel4</object-name>
    <object-name>panel5</object-name>
    <object-name>overhead</object-name>
    <object-name>sur-instrus</object-name>
    <object-name>rotor-brake</object-name>
    <object-name>power.sw</object-name>
    <object-name>vitre</object-name>
  </animation>

<animation>
    <object-name>panel1</object-name>
    <object-name>panel3</object-name>
    <object-name>panel4</object-name>
    <object-name>panel5</object-name>
    <object-name>overhead</object-name>
    <object-name>sur-instrus</object-name>
    <object-name>rotor-brake</object-name>
    <object-name>power.sw</object-name>
    <object-name>vitre</object-name>
    <condition>
      <not><property>/sim/rendering/rembrandt/enabled</property></not>
    </condition>
    <emission>
      <red>   0.01 </red>
      <green> 0.01 </green>
      <blue>  0.01 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

<animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Alouette-II/Models/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>



  <!-- Première rangée de gauche à droite -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Alt/alt.xml</path>
    <offsets>
      <x-m> -0.21962 </x-m>
      <y-m> -0.17669 </y-m>
      <z-m>  0.07909 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Vsi/vsi.xml</path>
    <offsets>
      <x-m> -0.23296 </x-m>
      <y-m> -0.07478 </y-m>
      <z-m>  0.07909 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Ai/ai.xml</path>
    <offsets>
      <x-m> -0.20880 </x-m>
      <y-m>  0.04405 </y-m>
      <z-m>  0.07793 </z-m>
    </offsets>
  </model>

<model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Ai/contour_ai.xml</path>
    <offsets>
      <x-m> -0.23578 </x-m>
      <y-m>  0.04426 </y-m>
      <z-m>  0.07787 </z-m>
    </offsets>
  </model>

  <model>
    <name>ASI_KMH</name>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Asi-metric/asi-metric.xml</path>
    <offsets>
      <x-m> -0.21937 </x-m>
      <y-m>  0.18616 </y-m>
      <z-m>  0.07584 </z-m>
    </offsets>
  </model>

  <animation>
    <type>select</type>
    <object-name>ASI_KMH</object-name>
    <condition>
      <equals>
        <property>sim/model/alouette/asi_mod</property>
        <value>false</value>
      </equals>
    </condition>
  </animation>

  <!-- Seconde rangée de gauche à droite -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Vor/VOR.xml</path>
    <offsets>
      <x-m> -0.21726 </x-m>
      <y-m> -0.16870 </y-m>
      <z-m> -0.02514 </z-m>
    </offsets>
  </model>

<model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Rotor/rotor.xml</path>
    <offsets>
      <x-m> -0.21726 </x-m>
      <y-m> -0.07559 </y-m>
      <z-m> -0.02210 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Hdg/hdg.xml</path>
    <offsets>
      <x-m> -0.21940 </x-m>
      <y-m>  0.04118 </y-m>
      <z-m> -0.02514 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Torque/torque.xml</path>
    <offsets>
      <x-m> -0.22127 </x-m>
      <y-m>  0.17388 </y-m>
      <z-m> -0.02514 </z-m>
    </offsets>
  </model>

<!-- Mini panneau-->

<model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Bille/bille.xml</path>
    <offsets>
      <x-m> -0.20995 </x-m>
      <y-m>  0.05955 </y-m>
      <z-m> -0.12276 </z-m>
    </offsets>
  </model>

  <!-- Panneau central bas -->
<model>
  <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/GNS430/GNS430.xml</path>
    <offsets>
      <x-m> -0.0801</x-m>
      <y-m> -0.0000 </y-m>
      <z-m> -0.3580 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg> -47.5 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>

  <!-- Magneto  -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Magneto/magneto.xml</path>
    <offsets>
      <x-m> -0.13471 </x-m>
      <y-m>  0.00201 </y-m>
      <z-m> -0.29663 </z-m>
      <pitch-deg> -26 </pitch-deg>
    </offsets>
  </model>
  <!-- Batterie  -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Battery/battery-switch.xml</path>
    <offsets>
      <x-m> -0.15792 </x-m>
      <y-m> -0.00181 </y-m>
      <z-m> -0.25078 </z-m>
      <pitch-deg> -26 </pitch-deg>
    </offsets>
  </model>


  <!-- Transpondeur -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/KT76A/kt76a.xml</path>
    <offsets>
      <x-m> -0.012 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.426 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>     -45 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

<!-- Radios -->

<!-- Comm -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Radio/ctl22-com.xml</path>
    <offsets>
      <x-m> 0.04464 </x-m>
      <y-m>  -0.04136 </y-m>
      <z-m> -0.46708 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>     -45 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

<!-- Nav -->
  <model>
    <path>Aircraft/Alouette-II/Models/Interior/Panel/Instruments/Radio/ctl32-nav.xml</path>
    <offsets>
