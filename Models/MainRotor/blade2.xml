<?xml version="1.0"?>

<PropertyList>

  <!-- Chaque pales est formée de 4 parties
  
       Axe 0               Axe 1               Axe 2               Axe 3
         +______blade1_______+_______blade2______+______blade3_______+_____blade4________
      center
        of
       rotor 

  -->
  <path>blade.ac</path>

  <animation>
    <object-name>blade1</object-name>
    <object-name>blade2</object-name>
    <object-name>blade3</object-name>
    <object-name>blade4</object-name>
  </animation>

  <animation>
    <name>ensemble3</name>
    <object-name>blade4</object-name>
  </animation>

  <animation>
    <name>ensemble4</name>
    <object-name>blade4</object-name>
    <object-name>blade3</object-name>
  </animation>

  <animation>
    <name>ensemble5</name>
    <object-name>blade4</object-name>
    <object-name>blade3</object-name>
    <object-name>blade2</object-name>
  </animation>

  <animation>
    <name>ensemble6</name>
    <object-name>blade4</object-name>
    <object-name>blade3</object-name>
    <object-name>blade2</object-name>
    <object-name>blade1</object-name>
  </animation>

  <!-- Axe 3 de pliage des pâles pour le cône -->
  <animation>
    <type>rotate</type>
    <object-name>ensemble3</object-name>
    <property>/rotors/main/cone4-deg</property>
    <factor> -1 </factor>
    <center>
       <x-m>  3.200 </x-m>
       <y-m>  0.011 </y-m>
       <z-m> -0.004 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Axe 2 de pliage des pâles pour le cône -->
  <animation>
    <type>rotate</type>
    <object-name>ensemble4</object-name>
    <property>/rotors/main/cone3-deg</property>
    <factor> -1 </factor>
    <center>
       <x-m>  2.340 </x-m>
       <y-m>  0.013 </y-m>
       <z-m> -0.003 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Axe 1 de pliage des pâles pour le cône -->
  <animation>
    <type>rotate</type>
    <object-name>ensemble5</object-name>
    <property>/rotors/main/cone2-deg</property>
    <factor> -1 </factor>
    <center>
       <x-m>  1.480 </x-m>
       <y-m>  0.015 </y-m>
       <z-m> -0.001 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Axe 0 de pliage des pâles pour le cône -->
  <animation>
    <type>rotate</type>
    <object-name>ensemble6</object-name>
    <property>/rotors/main/cone1-deg</property>
    <factor> -1 </factor>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Incidence de la pale -->
  <animation>
    <type>rotate</type>
    <object-name>ensemble6</object-name>
    <property>/rotors/main/blade[1]/incidence-deg</property>
    <factor>-1</factor>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

</PropertyList>

