<?xml version="1.0"?>

<PropertyList>

 <path>asi-metric.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation> 
  <type>rotate</type>
  <object-name>PointerCentre</object-name>
  <object-name>PointerTip</object-name>
  <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
    <entry>
      <ind> 0 </ind>
      <dep> 0 </dep>
    </entry>
    <entry>
      <ind> 26.998 </ind> <!-- 50 km/h -->
      <dep> 33.000 </dep>
    </entry>
    <entry>
      <ind>  53.996 </ind> <!-- 100 km/h -->
      <dep> 112.000 </dep>
    </entry>
    <entry>
      <ind>  80.994 </ind> <!-- 150 km/h -->
      <dep> 196.000 </dep>
    </entry>
    <entry>
      <ind> 107.992 </ind> <!-- 200 km/h -->
      <dep> 266.000 </dep>
    </entry>
    <entry>
      <ind> 134.990 </ind> <!-- 250 km/h -->
      <dep> 329.000 </dep>
    </entry>
  </interpolation>
  <axis> 
    <x> -1 </x>
    <y>  0 </y>
    <z>  0 </z>
  </axis>
  <center>
    <x-m> 0 </x-m>
    <y-m> 0 </y-m>
    <z-m> 0 </z-m>  
  </center>
 </animation>         
 
</PropertyList>
